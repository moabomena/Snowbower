pkgname=snowbower
pkgver=1.0
pkgrel=1
arch=(any)
source=()
url="https://github.com/moabomena/Snowbower"
pkgdesc='A program script to clear package managers caches'
license=(GPL3)

package() {
  install -d "$srcdir/$pkgname-$pkgver"
  install -Ddm755 ./snowbower "$pkgdir/usr/bin/snowbower"
}
